using System;
using System.Linq;
namespace Permutation
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] tab1 = new int[11];
            int[] tab2 = new int[11];

            for (int i = 0; i < 11; i++) tab1[i] = Convert.ToInt32(Console.ReadLine());
            for (int i = 0; i < 11; i++) tab2[i] = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine(Enumerable.SequenceEqual(tab1, tab2) == true? "YES" : "NO");
        }
    }
}
