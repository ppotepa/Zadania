using System;
using System.Collections.Generic;
using System.Linq;


namespace Powers
{
    class Program
    {
        public static IEnumerable<uint> powers(uint maxVal)
        {
            uint value = 1;
            for (var i = 0; value <= maxVal; i++)
            {
                yield return value;
                value = value * 2;                             
            }
        }

        static void Main(string[] args)
        {
            var strInput = Console.ReadLine();
            var split = strInput.Split(',').Select(UInt32.Parse).ToList();
            var max = split.Max();        
            var next_powers_of_two = powers(max);
            var as_string = string.Join(",", next_powers_of_two.Select(n => n.ToString()).ToArray());
            Console.WriteLine(as_string);

        }
        
    }
}
