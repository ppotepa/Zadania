using System;
using System.Collections.Generic;
using System.Linq;

namespace Program
{

    static class PrimeRange
    {

        static int getPrimesCount(int start, int end)
        {
            List<int> Primes = new List<int>();
            int current, i, ctr;
            for (current = start; current <= end; current++)
            {
                ctr = 0;

                for (i = 2; i <= current / 2; i++)
                {
                    if (current % i == 0)
                    {
                        ctr++;
                        break;
                    }
                }
                if (ctr == 0 && current != 1)
                    if(current > 1)Primes.Add(current);
            }
            return Primes.Count;
        }
        static void Main()
        {

            int T = Convert.ToInt32(Console.ReadLine());
            int test = 0;
            List<int> results = new List<int>();
            while (test < T)
            {
                int start = Convert.ToInt32(Console.ReadLine());
                int end = Convert.ToInt32(Console.ReadLine());
                results.Add(getPrimesCount(start, end));
                test++;
            }
            foreach (var result in results) Console.Write(result + " ");
        }
    }
}
